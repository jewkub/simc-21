<nav aria-label="Page navigation">
  <ul class="pagination justify-content-center">
    <%
      if (part != 1) {
    %>
    <li class="page-item">
      <a class="page-link" href="/exam?part=<%= part-1 %>" aria-label="Previous">
        <span aria-hidden="true"><i class="fas fa-angle-double-left"></i></span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
    <%
      }
      let sign = [, 1, 2, 3, 4, 5, 'ส่ง'];
      for (let i = 1; i <= 6; i++) {
    %>
      <li class="page-item <%= part == i ? 'active' : '' %>"><a class="page-link" href="<%= part == i ? '#' : '/exam?part=' + i %>"><%= sign[i] %></a></li>
    <%
      }
      if (part != 6) {
    %>
    <li class="page-item">
      <a class="page-link" href="/exam?part=<%= part+1 %>" aria-label="Next">
        <span aria-hidden="true"><i class="fas fa-angle-double-right"></i></span>
        <span class="sr-only">Next</span>
      </a>
    </li>
    <%
      }
    %>
  </ul>
</nav>