      $(() => {
        document.addEventListener("touchstart", function() {}, false);

        let cnt = 0;
        let chk = () => {
          if ($(window).scrollTop() < $('#shownav').offset().top && !($('#navbar .navbar-toggler').attr('aria-expanded') === 'true')) {
            $('#navbar, .bar').addClass('transparent');
 
            // debug for re-rendering in iOS
            if (!$('.debug').is(':animated') && cnt < 3) {
              $('.debug').animate({
                left: '+=1px',
                top: '0px',
              }, 200);
              cnt++;
            }
          }
          else {
            cnt = 0;
            $('#navbar, .bar').removeClass('transparent');
          }
        }
        window.chk = chk;
        $(window).scroll(chk);
        chk();

        // Every time a modal is shown, if it has an autofocus element, focus on it.
        $('.modal').on('shown.bs.modal', function () {
          $(this).find('[autofocus]').focus();
        });

        // Add slideDown and slideUp animation to Bootstrap dropdown.
        $('.dropdown').on('show.bs.dropdown', function() {
          $(this).find('.dropdown-menu').first().stop(true, true).slideDown();
        });
        $('.dropdown').on('hide.bs.dropdown', function() {
          $(this).find('.dropdown-menu').first().stop(true, true).slideUp();
        }); // https://stackoverflow.com/a/19339162/4468834


        $('#navbar').on('show.bs.collapse', function() {
          $('#navbar, .bar').removeClass('transparent');
        });
        $('#navbar').on('hidden.bs.collapse', function() {
          chk();
        }); // https://stackoverflow.com/a/22830975/4468834


