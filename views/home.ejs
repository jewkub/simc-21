    <style>
      @font-face {
        font-family: sukhumvit;
        src: url('/font/sukhumvit-set/SukhumvitSet-Medium.ttf');
      }
      @font-face {
        font-family: sukhumvit;
        src: url('/font/sukhumvit-set/SukhumvitSet-Bold.ttf');
        font-weight: bold;
      }

      ::selection {
        background: springgreen;
        color: black;
      }

      :root {
        font-size: 1.2em;
        --nav-height: 95px;
        --banner-height: 120px;
      }
      
      body {
        font-family: sukhumvit;
      }

      .bar {
        display: none;
        height: 16px;
        width: 100%;
        background-color: #eba486;
        position: absolute;
        top: 0;
        left: 0;
        transition: background-color 0.2s ease;
        /* right: 0; */
      }
      .bar.transparent {
        background-color: transparent;
      }

      .banner {
        /* height: var(--banner-height); */
        
        overflow-x: hidden; /* https://stackoverflow.com/a/6433475/4468834 */
      }

      .register-card {
        position: absolute;
        bottom: calc(1/4 * (100vh - var(--nav-height) - var(--banner-height)));
        right: 2.5vw;
        background-color: rgba(0, 0, 0, 0.5);
        border-radius: 20px;
        padding: 20px;
        color: white;
        
        
        display: flex;
        flex-flow: column nowrap;
      }

      .carousel-item {
        height: calc(100vh - var(--banner-height)); /* https://stackoverflow.com/a/36570618/4468834 */
        overflow: hidden;
      }
      .carousel-item img {
        height: calc(100vh - var(--banner-height));
        width: 100vw;
        object-fit: cover;
      }

      .carousel-control-next {
        left: 15%;
        right: auto;
      }

      #navbar {
        background-color: var(--gray);
        transition: background-color 0.2s ease;
        /* display: none;
        overflow-x: hidden;
        overflow-y: visible;
        padding-right: 100px; */
      }
      #navbar.transparent {
        /* visibility: hidden; */
        background-color: rgba(0, 0, 0, 0.33);
        
      }
      .navbar-brand {
        height: calc(var(--nav-height) - 8px - 8px);
      }

      .navbar-expand-lg .navbar-nav .nav-link {
        padding-left: 0.9rem;
        padding-right: 0.9rem;
        color: var(--white);
        transition: background-color 0.2s ease, color 0.2s ease;
      }
      .navbar-expand-lg .navbar-nav .nav-link:focus {
        color: white;
      }
      .navbar-expand-lg .navbar-nav .nav-link:hover {
        color: #d2e5da;
      }
      .navbar-expand-lg .navbar-nav .nav-link.active:focus {
        color: black;
      }
      .navbar-expand-lg .navbar-nav .nav-link.active:hover {
        color: #d2e5da;
      }
      .navbar-expand-lg a {
        outline: 0;
      }
      
      .dropdown:not(.show) .nav-link:hover {
        color: #d2e5da;
      }
      .transparent .nav-pills .show > .nav-link {
        background-color: transparent;
      }

      #banner-header {
        --left: -190px;
        position: relative;
        left: var(--left);
        overflow-x: hidden;
        width: calc(100% - var(--left));
      }

      .circle-crop {
        overflow: hidden;
        border-radius: 50%;
      }
      .circle-crop img {
         width: 100%;
         cursor: pointer;
         transition: filter 0.2s ease;
      }
      .circle-crop img:hover {
        /* opacity: 0.5; */
        filter: brightness(70%);
      }

      i {
        color: #eba486;
      }
      i.flaticon-arrow {
        display: none;
      }
      [class^="flaticon-"]::before {
        margin-left: 0px !important;
      }

      .icon::before {
        display: inline-block;
        font-style: normal;
        font-variant: normal;
        text-rendering: auto;
        margin-right: 0.75rem;
        -webkit-font-smoothing: antialiased;
        color: #eba486;
      }
      .call::before {
        font-family: "Flaticon";
        content: "\f100";
      }
      .mail::before {
        font-family: "Flaticon";
        content: "\f101";
      }
      .fb::before {
        font-family: "Flaticon";
        content: "\f104";
      }

      footer {
        background-color: var(--gray);
      }
      footer h1, footer h2, footer h3, footer h4, footer h5, footer h6, footer p, footer span {
        color: white;
      }
      #right {
        display: none;
      }

      a.anchor {
        display: block;
        position: relative;
        top: calc(0px - var(--nav-height));
        visibility: hidden;
        /* height: 0; */
      } /* https://stackoverflow.com/a/13184714/4468834 */

      .z-index-2 {
        z-index: 2;
      }

      @media only screen and (min-width: 768px) {
        #banner-header {
          position: static;
          width: initial;
        }

        i.flaticon-arrow {
          display: initial;
        }
        i.flaticon-arrow-down-sign-to-navigate {
          display: none;
        }
        #right {
          display: initial;
        }

        #gallery > div:nth-child(1) > div {
          margin: 0.6rem;
        }
        #gallery > div:nth-child(2) > div {
          margin-top: 6rem;
        }
        #gallery > div:nth-child(3) > div {
          margin: 0.4rem;
        }
        #gallery > div:nth-child(4) > div {
          margin-top: -5rem;
        }
        #gallery > div:nth-child(5) > div {
          margin-top: 2rem;
        }
        #gallery > div:nth-child(6) > div {
          margin-top: -5rem;
        }

        .swal2-container a {
          background-color: transparent;
          border-width: 2px;
          border-color: white;
        }
      }

      @media only screen and (min-width: 992px) {
        #navbar {
          /* display: flex; */
          visibility: initial;
        }
        .bar {
          display: initial;
        }
        .navbar-expand-lg .navbar-nav .nav-link {
          padding-top: 50px;
          margin-top: -50px;
          /* font-size: smaller; */
        }
        #navbar.transparent {
          visibility: initial;
        }
        
        #gallery > div:nth-child(1) > div {
          margin: 1rem;
        }
        #gallery > div:nth-child(2) > div {
          margin-top: 10rem;
        }
        #gallery > div:nth-child(3) > div {
          margin: 1rem;
        }
        #gallery > div:nth-child(4) > div {
          margin-top: -5rem;
        }
        #gallery > div:nth-child(5) > div {
          margin-top: 4rem;
        }
        #gallery > div:nth-child(6) > div {
          margin-top: -5rem;
        }
      }

      @media only screen and (min-width: 1200px) {
        .navbar-expand-lg .navbar-nav .nav-link {
          font-size: inherit;
        }
      }
    </style>
  </head>
